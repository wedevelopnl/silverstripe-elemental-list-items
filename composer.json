{
    "name": "wedevelopnl/silverstripe-elemental-list-items",
    "description": "List items module",
    "keywords": [
        "silverstripe",
        "list items",
        "usp",
        "usps"
    ],
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5.0",
        "unclecheese/display-logic": "^3.0",
        "symbiote/silverstripe-gridfieldextensions": "^4.0",
        "wedevelopnl/silverstripe-icon-manager": "^4.0"
    },
    "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.20"
    },
    "extra": {
        "installer-name": "silverstripe-elemental-list-items",
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "suggest": {
        "wedevelopnl/silverstripe-elemental-grid": "Our base elemental grid with customizable column widths to make the grid element work"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\ElementalListItems\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": true
        }
    }
}
